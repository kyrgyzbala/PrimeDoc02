<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kg.kyrgyzcoder.primedoc02">

    <application
        android:name=".util.PrimeDocDocApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_new_logo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_new_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:ignore="UnusedAttribute">
        <activity android:name=".ui.chat.CallActualActivity"></activity>
        <activity android:name=".ui.chat.CallUserActivity" />
        <activity android:name=".ui.chat.CallReceiveActivity" />
        <activity android:name=".ui.login.PinConfirmActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>